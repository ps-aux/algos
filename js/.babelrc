{
    "presets": [
        "@babel/preset-env",
        "@babel/typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver",
            {
                "extensions": [
                    ".js",
                    ".ts"
                ],
                "root": [
                    "."
                ]
            }
        ]
    ]
}
